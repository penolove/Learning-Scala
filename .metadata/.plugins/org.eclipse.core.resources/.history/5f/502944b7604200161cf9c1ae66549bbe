object www {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
	val nums = 1 ::2 ::3 ::4 ::Nil            //> nums  : List[Int] = List(1, 2, 3, 4)
	val qq= Nil.::(4).::(3).::(2).::(1)       //> qq  : List[Int] = List(1, 2, 3, 4)
	3::nums                                   //> res0: List[Int] = List(3, 1, 2, 3, 4)
}